<% if session[:user_id] %>
    <%= link_to "Log Out", logout_path, method: :delete %>
<% else %>
    <%= button_to "Create New User", register_path, method: :get %>
    <%= link_to "Log In", "/login", method: :get %>
<% end %>

<section class='existing-users'>
    <p>Existing Users: </p>
    <% @users.each do |user| %>
        <li><%= link_to user.email, user_path(user.id) %></li>
    <% end %>
</section>