<%= form_with url: login_path, method: :post, data: {turbo: false} do |form| %>
  <%= form.label :email %>
  <%= form.text_field :email %>
  <%= form.label :password %>
  <%= form.password_field :password %>
  <%= form.label :location %>
  <%= form.text_field :location, value: cookies.signed[:location]%>
  <%= form.submit "Log In" %>
<% end %>